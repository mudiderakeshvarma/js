//.................................Task--is  print HAPPY as HAIIY without using predefined functions

let a="happy";

let b=" ";

for(i=0; i<a.length; i++){

   if(a[i]=="p")
   {
   b=b+"i";
   }
   else
   {
    b=b+a[i];
   }
}
console.log(b);


