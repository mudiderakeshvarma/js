     function  rakesh(a,b){
if(c=="+")
 {
    console.log(a+b);
 }
 if(c== "-")
 {
    console.log(a-b);
 }
 if(c== "*")
 {
    console.log(a*b);
 }
 if(c== "%")
 {
    console.log(a%b);
 }
 if(c== "/")
 {
    console.log(a/b);
 }
 return a,b,c;
     }
var c=rakesh(10,20,"+")
 
