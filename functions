 function rakesh(a,b,c){

 if ((a>b) && (a>c))
   {
    console.log("a is greatter");
   }
   else if ((b>c) && (b>a))
   {
    console.log("b is greater")
   }
   else if ((c>b) && (c>a))
   {
    console.log("c is greater")
   }
   else if (a=b=c)
   {
    console.log("all are equal")
   }
    return a,b,c;
   }
   var avi=rakesh(3,7,2);

      

