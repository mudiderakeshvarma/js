function rakesh(a){
for (var a=1;a<=10;a++) 
   {
    alert(a);
    
    }

var a=1
while (a<=10)
{
    alert(a);
}


var a=1
do
{
    alert(a);
    a++
}
while(a<=10)

return a;
}
